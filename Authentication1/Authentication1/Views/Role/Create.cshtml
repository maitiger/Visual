@model List<WebApplication3.Areas.Identity.Data.CustomizeUser>

    @{
    ViewData["Title"] = "Create";
    }

    <h2>Create</h2>

    <div class="row">
        <div class="container">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>No.</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Roles</th>
                        <th>Handle</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var user in Model)
            {
                    <tr>
                        <td></td>
                        <td>@user.FirstName</td>
                        <td>@user.LastName</td>
                        <td>@user.Roles.Capacity</td>
                        <td>
                            <button>Edit</button>
                            <button>Delete</button>
                        </td>
                    </tr>
            }
                </tbody>
            </table>
        </div>
    </div>
